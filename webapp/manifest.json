{
  "_version": "1.12.0",
  "sap.app": {
    "id": "mobilefinance.MobileFinance",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "servicecatalog.connectivityComponentForManifest",
      "version": "0.0.0",
      "toolsId": "9b21b32c-7909-4730-9ae5-eaa7d31f4d5f"
    },
    "dataSources": {
      "ZFI_MOBILE_SRV": {
        "uri": "https://testapi-a4mel9cxg6.dispatcher.sa1.hana.ondemand.com/sap/opu/odata/sap/ZFI_MOBILE_SRV/",
        "type": "OData",
        "settings": {
          "localUri": "localService/metadata.xml"
        }
      },
      "ZFI_QUERY_BSP": {
        "uri": "https://testapi-a4mel9cxg6.dispatcher.sa1.hana.ondemand.com/sap(bD1lbiZjPTAzMg==)/bc/bsp/sap/zfi_query_data/query_execution.htm",
        "type": "http"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "mobilefinance.MobileFinance.view.Home",
      "type": "XML",
      "async": true,
      "id": "Home"
    },
    "dependencies": {
      "minUI5Version": "1.65.6",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.ui.unified": {},
        "sap.ui.comp": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": false
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "mobilefinance.MobileFinance.i18n.i18n"
        }
      },
      "finmobview": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Request",
          "useBatch": false
        },
        "dataSource": "ZFI_MOBILE_SRV",
        "preload": true
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "mobilefinance.MobileFinance.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "TargetHome",
          "pattern": "RouteHome",
          "target": "TargetHome"
        }
      ],
      "targets": {
        "TargetHome": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Home",
          "viewName": "Home",
          "controlAggregation": "detailPages"
        }
      }
    }
  },
  "sap.platform.abap": {
    "uri": "/sap/bc/ui5_ui5/sap/zmobile_finance/webapp",
    "_version": "1.1.0"
  }
}
