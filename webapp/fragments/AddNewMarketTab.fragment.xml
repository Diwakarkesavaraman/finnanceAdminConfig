<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core"
	xmlns:variant="sap.ui.comp.variants">
	<Dialog title="Add New Market Watch Tab" width="500em">
		<f:SimpleForm editable="true" class="" layout="ResponsiveGridLayout" columnsM="5" columnsL="5" columnsXL="5" labelSpanM="5" labelSpanL="5">
			<Label id="lblnmwtid" text="KPI ID"></Label>
			<Input id="inpnmwtid" value="" visible="false" required="true"></Input>
			<Label id="lblnmwtcat" text="Category"></Label>
			<Input id="inpnmwtcat" value="" required="true"></Input>
			<!--<Label id="lblnmwtitle" text="KPI Title"></Label>-->
			<!--<Input id="inpnmwtitle" value="" required="true"></Input>-->
			<Label id="lblnmwtrole" text="Roles"></Label>
			<Input id="inpnmwtrole" value=""></Input>
			<Label id="lblnmwtvis" text="Visibility"></Label>
			<!--<Input id="inpgisvis" value="" required="true"></Input>-->
			<CheckBox id="inpnmwtvis" text="" selected="" required="true"/>
		</f:SimpleForm>
		<buttons>
			<Button text="Cancel" type="Default" press="closeAddNewMarketTabDialog"/>
			<Button type="Emphasized" icon="sap-icon://add"  press="onAddMarketTab" text="Add"/>
			<!--<Button type="Reject" press="onclearfilter" text="Clear"/>-->
		</buttons>
	</Dialog>
</core:FragmentDefinition>