<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core"
	xmlns:variant="sap.ui.comp.variants">
	<Dialog title="Add Calendar Event Category" width="500em">
		<f:SimpleForm editable="true" class="" layout="ResponsiveGridLayout" columnsM="5" columnsL="5" columnsXL="5" labelSpanM="5" labelSpanL="5">
			<Label id="lblcalcatid" text="Category ID"></Label>
			<Input id="inpcalcatid" value="" visible="false" required="true"></Input>
			<Label id="lblcalcat" text="Category"></Label>
			<Input id="inpcalcat" value="" required="true"></Input>
		</f:SimpleForm>
		<buttons>
			<Button text="Cancel" type="Default" press="CloseCalCatDialog"/>
			<Button type="Emphasized" icon="sap-icon://add" press="onAddCalCat" text="Add"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>